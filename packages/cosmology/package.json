{
  "name": "cosmology",
  "version": "0.0.28",
  "description": "cosmology",
  "author": "Dan Lynch <pyramation@gmail.com>",
  "homepage": "https://github.com/pyramation/cosmology/tree/master/packages/cosmology#readme",
  "license": "SEE LICENSE IN LICENSE",
  "main": "main/index.js",
  "module": "module/index.js",
  "bin": {
    "cosmology": "main/cosmology.js"
  },
  "directories": {
    "lib": "src",
    "test": "__tests__"
  },
  "files": [
    "main",
    "module"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pyramation/cosmology"
  },
  "scripts": {
    "build:main": "cross-env BABEL_ENV=production babel src --out-dir main --delete-dir-on-start",
    "build:module": "cross-env MODULE=true babel src --out-dir module --delete-dir-on-start",
    "build": "npm run build:module && npm run build:main",
    "prepare": "npm run build",
    "lint": "eslint src --fix",
    "dev": "cross-env NODE_ENV=development babel-node src/cosmology",
    "mytest": "cross-env NODE_ENV=development babel-node src/my-test",
    "watch": "cross-env NODE_ENV=development babel-watch src/cosmology",
    "file": "cross-env NODE_ENV=development babel-watch src/file",
    "make:commands": "cross-env NODE_ENV=development node ./bin/build.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand"
  },
  "bugs": {
    "url": "https://github.com/pyramation/cosmology/issues"
  },
  "devDependencies": {
    "@babel/cli": "7.17.6",
    "@babel/core": "7.17.5",
