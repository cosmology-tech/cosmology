    "@keplr-wallet/unit": "0.9.12",
    "ansi-colors": "^4.1.1",
    "axios": "0.25.0",
    "class-autobind": "^0.1.4",
    "cosmjs-types": "0.4.1",
    "crypto-js": "4.1.1",
    "fuzzy": "0.1.3",
    "inquirerer": "0.1.3",
    "keychain": "1.3.0",
    "long": "5.2.0",
    "minimist": "1.2.5",
    "protobufjs": "6.11.2",
    "retry": "0.13.1",
    "secrets.js-grempe": "2.0.0",
    "tiny-async-pool": "^1.2.0"
  },
